   "source": [
    "# create all possible arrays shape of (3,3) where each element could be 1,0 or 2 \n",
    "import numpy as np\n",
    "a = [str(np.base_repr(t,base=3)).zfill(9) for t in range((3**3)**3)]\n",
    "c = np.array([np.array(list(b)).reshape(3,3) for b in a])\n",
    "c.astype(np.int)"
